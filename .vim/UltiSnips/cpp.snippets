snippet t "temp" b
case $1:
    output = "$2";
    break;
$0
endsnippet

snippet for "for loop" b
for ($1; $2; $3){
    $4
}
$0
endsnippet

snippet while "while loop" b
while (${1:condition}){
    $2
}
$0
endsnippet

snippet class "class definition" b
class $1 {
    private:
        $2
    public:
        $3
};
$0
endsnippet

snippet struct "struct definition" b
struct {
    $1
};
$0
endsnippet

snippet fun "function def" b
${1:out_type} ${2:name}($3){
    $4
}
$0
endsnippet

        
